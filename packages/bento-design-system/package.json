    "@babel/preset-typescript": "7.22.5",
    "@react-types/breadcrumbs": "3.4.4",
    "@react-types/button": "3.6.2",
    "@react-types/datepicker": "3.1.2",
    "@react-types/link": "3.3.4",
    "@react-types/menu": "3.7.2",
