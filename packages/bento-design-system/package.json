    "lint-staged": "13.0.3",
    "madge": "^5.0.1",
    "mini-css-extract-plugin": "2.6.1",
    "playroom": "0.31.0",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "smooth-release": "8.0.9",
    "storybook-addon-themes": "6.1.0",
    "style-loader": "3.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "9.3.1",
    "tsup": "7.1.0",
    "typescript": "4.7.4",
    "wait-on": "^7.0.1",
    "webpack": "5.76.3",
    "webpack-cli": "4.10.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "madge": {
    "tsConfig": "./tsconfig.json",
    "fileExtensions": [
      "ts",
      "tsx"
    ],
    "detectiveOptions": {
      "ts": {
        "skipTypeImports": true
      },
      "tsx": {
        "skipTypeImports": true
      }
    },
    "excludeRegExp": [
      "react-input-mask",
      "Menu/NestedMenu"
    ]
  }
}
